{
  "name": "ai-sdk-usage-insights",
  "version": "1.0.3",
  "type": "module",
  "description": "Usage logging middleware and dashboard companion for the AI SDK.",
  "main": "./dist/index.js",
  "bin": {
    "ai-sdk-usage": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -w -p tsconfig.json",
    "lint": "tsc -p tsconfig.json --noEmit",
    "test": "npm run lint",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai-sdk",
    "llm",
    "usage",
    "middleware",
    "analytics",
    "dashboard",
    "sqlite"
  ],
  "author": "Usage Insights Contributors",
  "license": "MIT",
  "files": [
    "dist",
    "views",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "better-sqlite3": "^12.2.0",
    "commander": "^14.0.0",
    "ejs": "^3.1.10",
    "ejs-mate": "^4.0.0",
    "express": "^5.1.0"
  },
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/notacoder/ai-sdk-usage-insights.git"
  },
  "bugs": {
    "url": "https://github.com/notacoder/ai-sdk-usage-insights/issues"
  },
  "homepage": "https://github.com/notacoder/ai-sdk-usage-insights#readme",
  "sideEffects": false,
  "engines": {
    "node": ">=18.17"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./dashboard": {
      "types": "./dist/dashboard/server.d.ts",
      "import": "./dist/dashboard/server.js",
      "default": "./dist/dashboard/server.js"
    },
    "./middleware": {
      "types": "./dist/usage-logger.d.ts",
      "import": "./dist/usage-logger.js",
      "default": "./dist/usage-logger.js"
    },
    "./package.json": "./package.json"
  }
}
